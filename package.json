{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "description": "Federalist using USWDS+Jekyll",
  "scripts": {
    "build": "npm run build:uswds; npm run build:jekyll",
    "build:jekyll": "bundle exec jekyll build",
    "build:uswds": "ruby build.rb uswds;",
    "clean": "ruby build.rb clean",
    "federalist": "npm run build:uswds;",
    "postinstall": "bundle install",
    "start": "npm run build; bundle exec jekyll serve;",
    "test": "bundle exec htmlproofer _site; npx a11y '_site/**/*.html'"
  },
  "author": "",
  "license": "CC0-1.0",
  "dependencies": {
    "a11y": "^0.5.1",
    "uswds": "^2.0.2"
  }
}
